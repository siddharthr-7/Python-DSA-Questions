   "source": [
    "def sum_of_elements(lst):\n",
    "    total = 0\n",
    "    for num in lst:\n",
    "        total += num\n",
    "    return total\n",
    "\n",
    "# Example usage\n",
    "my_list = [3, 1, 4, 1, 5]\n",
    "result = sum_of_elements(my_list)\n",
    "print(\"Sum of all elements:\", result)\n"
   ]
