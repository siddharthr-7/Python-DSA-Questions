        "lst = [2,[1,2,3],6,[7,[9,10]]]\n",
        "\n",
        "def count_ele(lst):\n",
        "  count = 0\n",
        "  for item in lst:\n",
        "    if isinstance(item,list):\n",
        "      count += count_ele(item)\n",
        "    else:\n",
        "      count += 1\n",
        "  return count\n",
        "print(count_ele(lst))"
