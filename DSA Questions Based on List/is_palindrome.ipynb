   "source": [
    "def is_pal(lst):\n",
    "    left = 0\n",
    "    right = len(lst)-1\n",
    "\n",
    "    while left < right:\n",
    "        if lst[left] != lst[right]:\n",
    "            return False # Mismatch found\n",
    "        left +=1\n",
    "        right -=1\n",
    "    return True # All matched\n",
    "\n",
    "lst = [1,2,3,2,1]\n",
    "print(\"Is Palindrome :\",is_pal(lst))"
   ]
